{
  "name": "vue-crontab",
  "version": "0.0.1",
  "description": "Periodic processing management system (crontab) running on Vue.js",
  "main": "dist/VueCrontab.common.js",
  "module": "dist/vue-crontab.esm.js",
  "unpkg": "dist/vue-crontab.js",
  "jsdelivr": "dist/vue-crontab.js",
  "scripts": {
    "dev-server": "./node_modules/.bin/cross-env NODE_ENV=development webpack-dev-server --config examples/webpack.config.js --open --hot",
    "test:unit": "./node_modules/.bin/ts-node --project tsconfig.json ./node_modules/jasmine/bin/jasmine JASMINE_CONFIG_PATH=test/unit/support/jasmine.json",
    "test:e2e": "./node_modules/.bin/wdio ./test/e2e/wdio.conf.js",
    "build": "./node_modules/.bin/ts-node build/build.ts module",
    "build:test": "node dist/vue-crontab.js && node dist/vue-crontab.min.js && node dist/vue-crontab.common.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hirasaki1985/vue-crontab.git"
  },
  "author": "Mamoru Hirasaki <hirasaki1985@gmail.com> (https://walking-succession-falls.com/)",
  "keywords": [
    "vue",
    "crontab"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jasmine": "^3.3.8",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "es6-promise": "^4.2.5",
    "eslint": "^5.13.0",
    "jasmine": "^3.3.1",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-root-import": "^0.2.3",
    "rollup-plugin-typescript2": "^0.18.1",
    "rollup-plugin-uglify-es": "0.0.1",
    "selenium-standalone": "^6.15.4",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "typescript": "^3.3.1",
    "vue": "^2.5.22",
    "vue-loader": "^15.6.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.22",
    "vuex": "^3.1.0",
    "wdio-jasmine-framework": "^0.3.8",
    "wdio-selenium-standalone-service": "0.0.10",
    "webdriverio": "^4.14.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
